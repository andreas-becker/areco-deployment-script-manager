        <widget id="createWorkflowActionAdapter" widgetDefinitionId="com.hybris.cockpitng.workflow.create" slotId="cockpitWidgetChildrenInvisible" template="false">
            <setting key="widgetStyleClass" type="String"></setting>
            <setting key="__externalLocationPath" type="String">extension://platformbackoffice</setting>
            <setting key="widgetStyleAttribute" type="String"></setting>
            <virtual-sockets/>
        </widget>

        <setting key="widgetStyleClass" type="String"></setting>
        <setting key="widgetStyleAttribute" type="String"></setting>
        <virtual-sockets />

    </widget-extension>

    <widget-connection sourceWidgetId="explorerTree" outputId="nodeSelected" targetWidgetId="run_deployment_scripts_conditionevaluator"
                       inputId="input" />
    <widget-connection sourceWidgetId="run_deployment_scripts_conditionevaluator" outputId="true" targetWidgetId="run_deployment_scripts_propertyextractor"
                       inputId="genericInput" />
    <widget-connection sourceWidgetId="run_deployment_scripts_propertyextractor" outputId="genericOutput" targetWidgetId="configurableFlow"
                       inputId="contextType" />
</widgets>
