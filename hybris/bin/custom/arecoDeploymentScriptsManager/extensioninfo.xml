<extensioninfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="extensioninfo.xsd">

   <extension  name="arecoDeploymentScriptsManager"
               abstractclassprefix="Generated"
               classprefix="ArecoDeploymentScriptsManager"
               managername="ArecoDeploymentScriptsManagerManager"
               managersuperclass="de.hybris.platform.jalo.extension.Extension"
               >
               
      <!-- These dependencies are required by Hybris 5.x -->
	  <requires-extension name="hmc"/>
	  <requires-extension name="cockpit"/>

	   <!-- The default backoffice configuration of the extension backoffice uses Hybris type from this extension -->
	   <requires-extension name="lucenesearch" />
	   <!-- The default backoffice spring configuration of the extension backoffice uses the platformEnumValueResolver bean type from this extension -->
	   <requires-extension name="platformbackoffice" />

      <coremodule	packageroot="org.areco.ecommerce.deploymentscripts"
					manager="org.areco.ecommerce.deploymentscripts.jalo.ArecoDeploymentScriptsManagerManager"
					generated="true" 
					/>
      <hmcmodule	extensionclassname="org.areco.ecommerce.deploymentscripts.hmc.ArecoDeploymentScriptsManagerHMCExtension"
					/>
	<!-- Required by Hybris 6.x -->
	<meta key="backoffice-module" value="true"/>
		
   </extension>
      
</extensioninfo>
